<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.vyatsu.FXMLDocumentController">
    <TableView fx:id="tableProducts" layoutX="8.0" prefHeight="337.0" prefWidth="773.0">
        <columns>
            <TableColumn fx:id="idCol" prefWidth="55.0" text="№"/>
            <TableColumn fx:id="titleCol" prefWidth="175.0" text="Название"/>
            <TableColumn fx:id="priceCol" prefWidth="150.0" text="Цена"/>
            <TableColumn fx:id="nameCol" prefWidth="150.0" text="Категория"/>
        </columns>
    </TableView>
    <Button fx:id="loadButton" layoutX="327.0" layoutY="350.0" onAction="#handleButtonAction" prefHeight="25.0"
            prefWidth="128.0" text="Загрузить продукты"/>
</AnchorPane>
